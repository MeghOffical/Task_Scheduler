{
  "$schema": "../../node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "_comment": "Mutation testing configuration for reset-password module",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "json"
  ],
  "testRunner": "jest",
  "jest": {
    "projectType": "custom",
    "configFile": "jest.config.js",
    "enableFindRelatedTests": true
  },
  "coverageAnalysis": "off",
  "mutate": [
    "src/app/reset-password/page.tsx"
  ],
  "disableTypeChecks": "{src,lib}/**/*.{js,ts,jsx,tsx}",
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  },
  "checkers": [],
  "tempDirName": "testing/mutation/stryker-tmp-reset-password",
  "htmlReporter": {
    "fileName": "testing/mutation/reports/reset-password-mutation-report.html"
  },
  "jsonReporter": {
    "fileName": "testing/mutation/reports/reset-password-mutation-report.json"
  },
  "ignorePatterns": [
    "node_modules",
    ".next",
    "testing"
  ]
}
