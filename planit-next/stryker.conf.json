{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "_comment": "Mutation testing configuration for planit-next",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "json"
  ],
  "testRunner": "jest",
  "jest": {
    "projectType": "custom",
    "configFile": "jest.stryker.config.js",
    "enableFindRelatedTests": true
  },
  "coverageAnalysis": "off",
  "mutate": [
    "src/app/pomodoro/page.tsx"
  ],
  "comment": "Add more files here as you write tests for them. Avoid files with complex dependencies or TypeScript errors.",
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 40
  },
  "checkers": [],
  "tempDirName": "testing/mutation/stryker-tmp",
  "htmlReporter": {
    "fileName": "testing/mutation/reports/mutation-report.html"
  },
  "jsonReporter": {
    "fileName": "testing/mutation/reports/mutation-report.json"
  },
  "ignorePatterns": [
    "node_modules",
    ".next",
    "testing/mutation/stryker-tmp"
  ]
}
